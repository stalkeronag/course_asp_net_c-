@page
@model Dotnet.Web.Admin.Pages.RegisterPageModel

<form method="post">
<div>
    <div class="form-group">
        <label asp-for="Email">Введите Email</label>
        <input type="text" name="Email" />
    </div>
    <div class="form-group">
        <label asp-for="Password">Введите пароль</label>
        <input type="text" name="Password" />
    </div>
    <div class="form-group">
        <label asp-for="UserName">Введите имя</label>
        <input type="text" name="UserName" />
    </div>
    <div class="form-group">
        <input type="submit" value="Зарегистрироваться" class="btn btn-outline-dark" />
    </div>
</div>
</form>
